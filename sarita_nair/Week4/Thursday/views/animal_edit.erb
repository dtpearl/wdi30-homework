<h2>Edit Animals</h2>
<form action="/animals/<%= @results["id"] %>" method="post">
  <label>Name</label>
  <input type="text" name="name" placeholder="name" value=<%= @results["name"] %>>
  <label>Class</label>
  <input type="text" name="class" placeholder="class" value=<%= @results["class"] %>>
  <label>URL</label>
  <input type="text" name="image" placeholder="http://" value=<%= @results["image"] %>>
  <button>Update</button>
</form>
