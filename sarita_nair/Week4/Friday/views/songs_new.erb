<h2>Add Songs</h2>
<form action="/songs/new" method="post">
  <input type="text" name="name" placeholder="Singer Name">
  <input type="text" name="url" placeholder="<iframe">
  <select name="artist_id">
    <% Singer.all.each do |singer| %>
    <option value="<%= singer.id %>">
      <%= singer.name %>
    </option>
    <% end %>
  </select>
<button>Create</button>
</form>
